{
  "manifest_version": 2,
  "name": "Go To Admin",
  "version": "1.3",
  "description": "Fast open admin panels",

  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/512.png",
    "default_popup": "popup/index.html"
  }
}
