{
  "manifest_version": 2,
  "name": "Go To Admin",
  "version": "1.3",
  "description": "Fast open admin panels",

  "icons": {
    "48": "icons/ico-48.png"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icons/ico-48.png",
    "default_popup": "popup/index.html"
  }
}
